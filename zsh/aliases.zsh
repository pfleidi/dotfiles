# ZSH Aliases

alias ll='ls -ahl'
alias lt='ls -ahltr'
alias ducks='du -cks * | sort -rn |head -11'
alias grep='grep --color=auto'
alias less="less -r"
alias more="less -r"
alias ut='bsdtar -vx -f'
alias spk='RAILS_ENV=test COVERAGE=1 bin/spring rspec'
alias cuke='RAILS_ENV=test COVERAGE=1 bin/spring cucumber --format progress'
alias vi=vim

